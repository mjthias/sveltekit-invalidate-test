{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/([^/]+?)/?$",
			"dest": "/[slug]?__pathname=/$1"
		},
		{
			"src": "^/([^/]+?)/?/__data.json$",
			"dest": "/[slug]/__data.json?__pathname=/$1"
		},
		{
			"src": "/.*",
			"dest": "/fn"
		}
	],
	"overrides": {}
}